{
  "name": "zwell/qdrant",
  "description": "PHP Client for Qdrant",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": "^7.4",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0|^2.0",
    "psr/log": "^1.0|^2.0|^3.0",
    "guzzlehttp/guzzle": "^6.3",
    "guzzlehttp/psr7": "^1.9.0",
    "webmozart/assert": "^1.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.5",
    "openai-php/client": "^0.3.5",
    "hkulekci/cohere": "dev-main",
    "phpunit/php-code-coverage": "^10.1@dev"
  },
  "autoload": {
    "psr-4": {
      "Qdrant\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Qdrant\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "zwell",
      "email": "zhaweichang@gmail.com"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit --display-warnings --display-deprecations",
    "coverage": "vendor/bin/phpunit --display-warnings --display-deprecations --coverage-html coverage/"
  }
}